{
  "name": "vanilla-tailwind-test",
  "version": "1.0.0",
  "description": "This is a repo to test using vanilla JS, Express JS, and tailwindCSS for a small web application.",
  "main": "index.js",
  "scripts": {
    "start": "run-s tab:dev tab:build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node index.js",
    "build": "npx tailwindcss -i ./public/tailwind.css -o ./public/styles.css --watch",
    "tab:dev": "ttab -t 'Start Server' npm run dev",
    "tab:build": "ttab -t 'Tailwind Build' npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youngbloodcyb/vanilla-tailwind-test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/youngbloodcyb/vanilla-tailwind-test/issues"
  },
  "homepage": "https://github.com/youngbloodcyb/vanilla-tailwind-test#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.1",
    "ttab": "^0.7.3"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
